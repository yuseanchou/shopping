/**index.wxss**/
.index {
  color: var(--themcolor);
}
.index swiper image {
  width: 100%;
}
.index .nav {
  display: flex;
}
.index .nav navigator { 
  flex: 1;
}
.index .nav navigator image {
  width: 100%;
}
.index .floorList .product_list navigator {
  float: left;
  width: 33%;
}
.index .floorList .product_list navigator:nth-last-child(-n + 4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.index .floorList .product_list navigator:nth-last-child(2),
.index .floorList .product_list navigator:nth-last-child(3) {
  border-bottom: 10rpx solid #fff;
}
.index .floorList .product_list navigator image {
  width: 100%;
  height: 100%;
}
